#!/usr/bin/env bash

# Loop through the files in the current directory
for file in *; do
  # Check if the file name starts with a dash
  if [[ $file == -* ]]; then
    # Extract the part of the name after the first dash
    name=$(echo $file | cut -d'-' -f2-)
    # Print the name in list format
    echo "- $name"
  fi
done
